{
    "name": "eti2frontend",
    "version": "0.0.1",
    "scripts": {
        "dev": "svelte-kit dev",
        "build": "svelte-kit build",
        "build:dev": "cross-env FOR_DEV=true svelte-kit build",
        "build:gh": "cross-env TO_GITHUB=true svelte-kit build",
        "package": "svelte-kit package",
        "preview": "svelte-kit preview --port 9160",
        "check": "svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
        "format": "prettier --write --plugin-search-dir=. ./src",
        "format:all": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
        "deploy:gh": "node -r dotenv/config ./scripts/publish-gh-pages.mjs dotenv_config_path=./scripts/.env"
    },
    "devDependencies": {
        "@sveltejs/adapter-node": "^1.0.0-next.56",
        "@sveltejs/adapter-static": "^1.0.0-next.22",
        "@sveltejs/kit": "next",
        "@tailwindcss/forms": "^0.4.0",
        "@tailwindcss/typography": "^0.5.1",
        "autoprefixer": "^10.4.2",
        "cross-env": "^7.0.3",
        "cssnano": "^5.0.16",
        "dotenv": "^16.0.0",
        "gh-pages": "^3.2.3",
        "postcss": "^8.4.6",
        "postcss-load-config": "^3.1.1",
        "prettier": "^2.5.1",
        "prettier-plugin-svelte": "^2.6.0",
        "svelte": "^3.46.4",
        "svelte-check": "^2.4.2",
        "svelte-preprocess": "^4.10.2",
        "svelte-time": "^0.6.1",
        "tailwindcss": "^3.0.18",
        "tslib": "^2.3.1",
        "typescript": "^4.5.5",
        "vite": "^2.8.0"
    },
    "type": "module",
    "dependencies": {
        "@steeze-ui/heroicons": "^1.0.2",
        "@steeze-ui/svelte-icon": "^1.2.2",
        "@types/prettier": "^2.4.3",
        "axios": "^0.25.0",
        "svelte-use-click-outside": "^1.0.0"
    }
}
